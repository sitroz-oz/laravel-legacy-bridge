{
    "name": "sitroz/laravel-legacy-bridge",
    "description": "This package allows you to use all the familiar and powerful functionality of laravel as a library for an existing project",
    "type": "library",
    "version": "0.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Zaitsev",
            "email": "sitroz@live.ru"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sitroz\\LaraBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sitroz\\LaraBridge\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~5.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraBridgeServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
